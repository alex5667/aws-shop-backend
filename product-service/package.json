{
  "name": "product-service",
  "version": "1.0.0",
  "description": "product-service-api",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "prettier": "prettier src --write",
    "cdk": "cdk",
    "cdk:bs": "npx cdk bootstrap --profile myprofile",
    "cdk:deploy": "npx cdk deploy --profile myprofile",
    "cdk:destroy": "npx cdk destroy --profile myprofile",
    "db:seed": "ts-node src/lib/seed.ts --profile myprofile"
  },
  "keywords": [
    "aws",
    "node",
    "lambda"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.110.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.110.0-alpha.0",
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/util-dynamodb": "^3.454.0",
    "@types/aws-lambda": "^8.10.126",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "aws-cdk": "^2.110.0",
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "joi": "^17.11.0"
  }
}
